{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": [
      "error", {
        "devDependencies": true
      }
    ],
    "quote-props": [
      "error",
      "consistent"
    ]
  }
}
